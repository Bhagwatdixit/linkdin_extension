{
  "manifest_version": 3,
  "name": "LinkedIn Extensions",
  "description": "Voice assistant Extension",
  "version": "1.0",
 
  "icons": {
    "16": "images/01_icon.png",
    "32": "images/02_icon.png",
    "48": "images/03_icon.png",
    "128": "images/04_icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "css": ["scripts/content.css"],
      "html": ["scripts/popup.html"],
      "matches": ["https://www.linkedin.com/*"
    ]
      
    }
  ],
  "web_accessible_resources":[{
    "resources": ["images/save.png"],
    "matches": ["https://www.linkdin.com/*"]
  }]
}


